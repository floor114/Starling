<% provide(:title, 'Users') %>

<div class='container clearfix'>
  <div class='content'>

    <div class='wall-users'>
      <div class='artcl'>
        Users
      </div>
      <hr>
      <% @users.each do |user| %>
          <% if user.id != current_user.id%>
              <%= render 'user', user: user %>
          <% end %>
      <% end %>
      <div class="pagination">
        <%= will_paginate @users %>
      </div>
    </div>
  </div>
</div>


