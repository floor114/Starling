<%= form_for(:user, :url => registration_path(resource_name)) do |f| %>
    <%= f.text_field :name, autocomplete: "off", :placeholder => "Full name"%>
    <%= f.email_field :email, autocomplete: "off", :placeholder => "Email" %>
    <%= f.password_field :password, autocomplete: "off" , :placeholder => "Password"%>
    <%= f.password_field :password_confirmation, autocomplete: "off", :placeholder => "Password confirmation" %>
    <%= f.button "Log in for Starling" %>
<% end %>